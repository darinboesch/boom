{
  "name": "format-file-name",
  "description": "returns a new file name for a given file name {sourceFile}",
  "dryRun": true,
  "execStack": [
    { "item": "sourceFile" },
    { "item": "stack.pushOptionValue", "type": "operator" },
    { "item": "R-\\d*_", "type": "regex" },
    { "item": "regex.test", "type": "operator" },
    { "item": "stack.condition.ne.exitNoAction", "type": "operator" },
    { "item": "sourceFile" },
    { "item": "stack.pushOptionValue", "type": "operator" },
    { "item": "(?<=R-\\d*_)\\d{2}", "type": "regex", "comment": "year" },
    { "item": "regex.match", "type": "operator" },
    { "item": "sourceFile" },
    { "item": "stack.pushOptionValue", "type": "operator" },
    { "item": "(?<=R-\\d*_\\d{2})\\d{2}", "type": "regex", "comment": "month" },
    { "item": "regex.match", "type": "operator" },
    { "item": "sourceFile" },
    { "item": "stack.pushOptionValue", "type": "operator" },
    { "item": "(?<=R-\\d*_\\d{4})\\d{2}", "type": "regex", "comment": "day" },
    { "item": "regex.match", "type": "operator" },
    { "item": "sourceFile" },
    { "item": "stack.pushOptionValue", "type": "operator" },
    { "item": "(?<=R-\\d*_\\d{6})\\d*", "type": "regex", "comment": "time" },
    { "item": "regex.match", "type": "operator" },
    { "item": "20${0}-${1}-${2}-${3}" },
    { "item": "string.sprintf", "type": "operator" },
    { "item": "sourceFile" },
    { "item": "stack.pushOptionValue", "type": "operator" },
    { "item": "stack.rotate", "type": "operator" },
    { "item": "R-\\d*_\\d*", "type": "regex", "comment": "replace string argument" },
    { "item": "stack.rotate", "type": "operator" },
    { "item": "string.replace", "type": "operator" }
  ]
}
